{
	"name": "lua-blog-static",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"node-sass": "^4.14.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-feather": "2.0.8",
		"react-helmet": "^6.1.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.1",
		"react-snapshot": "^1.3.0"
	},
	"devDependencies": {
		"@types/node": "^12.0.0",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@types/react-helmet": "^6.1.0",
		"@types/react-router-dom": "^5.1.5",
